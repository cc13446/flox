<?xml version = "1.0" ?>
<!DOCTYPE root SYSTEM "script.dtd">
<root>
    update data_source
    <set>
        <if test="type != null">
            type = #{type},
        </if>
        <if test="url != null">
            url = #{url},
        </if>
        <if test="username != null">
            username = #{username},
        </if>
        <if test="password != null">
            password = #{password},
        </if>
        <if test="status != null">
            status = #{status},
        </if>
        <if test="config != null">
            config = #{config},
        </if>
        <if test="attribute != null">
            attribute = #{attribute},
        </if>
    </set>
    <where>
        code = #{code}
    </where>
</root>